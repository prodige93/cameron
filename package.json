{
  "name": "toiture-pro",
  "version": "1.0.0",
  "description": "Site web pour entreprise de couverture - Toiture Pro",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.4.0"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port ${PORT:-3000}",
    "build": "vite build",
    "generate-prerender": "node scripts/generate-prerender.js",
    "preview": "vite preview --host 0.0.0.0 --port ${PORT:-3000}",
    "start": "vite preview --host 0.0.0.0 --port ${PORT:-3000}",
    "render": "npm run build && npm start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

